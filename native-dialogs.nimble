[Package]
name          = "native-dialogs"
version       = "0.1.0"
author        = "Rostyslav Dzinko (rostislav.dzinko@gmail.com)"
description   = "Implements framework-agnostic native operating system dialogs calls"
license       = "MIT"
srcDir        = "src"

[Deps]
